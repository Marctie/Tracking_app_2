{
  "apiBaseUrl": "https://production-api.yourcompany.com/api",
  "mqttBrokerUrl": "wss://production-mqtt.yourcompany.com/ws",
  "environment": "production",
  "appVersion": "1.0.0",
  "features": {
    "realTimeUpdates": true,
    "satelliteView": true,
    "autoRefreshInterval": 10000
  },
  "api": {
    "endpoints": {
      "vehicles": "/Vehicles",
      "positions": "/positions", 
      "users": "/users",
      "auth": "/Auth"
    },
    "timeout": 30000,
    "retryAttempts": 3
  },
  "mqtt": {
    "hostname": "production-mqtt.yourcompany.com",
    "port": 443,
    "path": "/ws",
    "protocol": "wss",
    "username": "your-prod-username",
    "password": "your-prod-password",
    "keepalive": 120,
    "topics": {
      "vehicleStatus": "vehicles/+/status",
      "vehiclePosition": "vehicles/+/position"
    }
  },
  "map": {
    "defaultCenter": {
      "latitude": 41.9028,
      "longitude": 12.4964
    },
    "defaultZoom": 12,
    "maxZoom": 18,
    "minZoom": 4
  }
}