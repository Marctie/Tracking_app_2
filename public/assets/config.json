{
  "apiBaseUrl": "http://10.0.90.9/Stage/TrackingApp",
  "mqttBrokerUrl": "wss://rabbitmq.test.intellitronika.com/ws",
  "appVersion": "1.0.0",
  "features": {
    "realTimeUpdates": true,
    "autoRefreshInterval": 5000
  },
  "api": {
    "endpoints": {
      "vehicles": "/api/Vehicles",
      "positions": "/api/positions", 
      "users": "/api/users",
      "auth": "/api/Auth"
    },
    "timeout": 30000,
    "retryAttempts": 3
  },
  "map": {
    "defaultCenter": {
      "latitude": 41.9028,
      "longitude": 12.4964
    },
    "defaultZoom": 12,
    "maxZoom": 18,
    "minZoom": 4
  }
}